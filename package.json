{
  "name": "blog_v2",
  "version": "1.0.0",
  "author": "Taichi Tsujioka",
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf .next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint '**/*.{tsx,ts,js,jsx}' --ignore-path .eslintignore",
    "fix": "yarn format && yarn lint:fix",
    "format": "prettier --write '**/*.{tsx,ts,js,jsx}' --ignore-path .prettierignore",
    "lint:fix": "eslint --fix '**/*.{tsx,ts,js,jsx}' --ignore-path .eslintignore",
    "typecheck": "tsc --noEmit --pretty",
    "extract": "formatjs extract '{components,hooks,pages}/**.{tsx,ts}' --out-file ./lang/en-US.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format simple",
    "generate": "DOTENV_CONFIG_PATH='.env.development' graphql-codegen --require dotenv/config --config codegen.yml"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.2.9",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/typescript-graphql-request": "^3.1.1",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "ts-loader": "8.1.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@formatjs/ts-transformer": "^3.3.9",
    "@prefresh/next": "^1.4.6",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "next": "^10.2.0",
    "preact": "^10.5.5",
    "preact-render-to-string": "^5.1.11",
    "react": "npm:@preact/compat@^0.0.3",
    "react-dom": "npm:@preact/compat@^0.0.3",
    "react-intl": "^5.17.1",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.1.2"
  },
  "license": "MIT"
}
